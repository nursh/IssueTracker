{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "faker": "^4.1.0",
    "formik": "^2.1.4",
    "http-proxy-middleware": "^1.0.3",
    "is-ci": "^2.0.0",
    "is-ci-cli": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "postcss-cli": "^7.1.0",
    "query-string": "^6.12.1",
    "react": "^16.12.0",
    "react-avatar": "^3.9.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^1.2.0",
    "yup": "^0.28.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:styles": "postcss src/tailwind.css -o src/index.css",
    "prebuild": "yarn build:styles",
    "prestart": "yarn build:styles",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "server-start": "npm --prefix ../server run test:start",
    "app:start": "concurrently \"yarn start\" \"yarn:server-start\"",
    "pretest:e2e:run": "yarn run build",
    "test:e2e:run": "start-server-and-test http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test app:start http://localhost:3000 cy:open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "start-server-and-test": "^1.11.0"
  },
  "peerDependencies": {
    "cypress": "^4.5.0"
  }
}
